# NASA AI Agents Environment Configuration
# Copy this file to .env and add your API keys

# OpenAI API Key (required for all agents)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Organization/Project IDs (if your account requires them)
OPENAI_ORG_ID=org-your_org_id_here
OPENAI_PROJECT_ID=proj_your_project_id_here

# Optional: Customize model settings
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.1

# Optional: Gradio settings
GRADIO_SHARE=False
GRADIO_SERVER_NAME=0.0.0.0

# Demo settings
DEMO_MODE=True